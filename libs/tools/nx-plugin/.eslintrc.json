{
  "extends": ["../../../.eslintrc.cjs"],
  "ignorePatterns": ["!**/*", "node_modules/**/*"],
  "rules": {
    "import/no-extraneous-dependencies": "off"
  },
  "overrides": [
    {
      "files": ["*.json"],
      "parser": "jsonc-eslint-parser",
      "rules": {
        "@nx/dependency-checks": "error"
      }
    }
  ]
}
